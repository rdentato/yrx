@CCurveArrow bias {0.25c} from { C } to { D } ylabel { 1.2f @Font {@Sym epsilon }}
@CurveArrow  bias {0.25c} from { F } to { D } ylabel { 1.2f @Font {@Sym epsilon }} ylabelprox {below } 
@Arrow from { D } to { E } ylabel { 1.2f @Font {@Sym epsilon "/\)" }}
}
                              
