@Diag {
@Tbl
aformat { @Cell A | @Cell B | @Cell C | @Cell D | @Cell i {ctr} E | @Cell F | @Cell G | @Cell H | @Cell I }
marginhorizontal { 0.3c }
marginvertical { 0.2c }
{
@Rowa
