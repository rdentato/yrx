G { D:: @Circle hsize {0.5f} {0.75f @Font {3}}}
I { E:: @Circle hsize {0.5f} {0.75f @Font {4}}}
@Rowa
E { (X)? }
}
//
@Arrow from { A } to { B } ylabel { 1.2f @Font {@Sym epsilon "/\(" }}
