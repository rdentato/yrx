}
//
@Arrow from { A } to { B } ylabel { 1.2f @Font {@I a}}
@Arrow from { B } to { C } ylabel { 1.2f @Font {@I b}}
}
@Diag {
@Tbl
aformat { @Cell A | @Cell i {ctr} B | @Cell C }
marginhorizontal { 0.3c }
marginvertical { 0.2c }
{
@Rowa
A { A:: @Circle hsize {0.5f} {0.75f @Font {1}}}
C { B:: @Circle hsize {0.5f} {0.75f @Font {2}}}
@Rowa
